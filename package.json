{
  "name": "@8hobbies/typedoc-plugin-404",
  "version": "1.0.0",
  "description": "Create a 404 page for typedoc-generated doc.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist",
    "!/dist/**.test.d.ts",
    "!/dist/**.test.js"
  ],
  "scripts": {
    "clean": "tsc --build --clean",
    "compile": "tsc",
    "doc": "typedoc",
    "format": "eslint --fix --ext .ts . && prettier . --write",
    "lint": "eslint --report-unused-disable-directives --max-warnings=0 --ext .ts . && prettier . --check",
    "prepack": "npm run clean && npm run compile",
    "test": "npm run compile",
    "test-all": "npm run compile && npm run lint && npm run test && npm run doc"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/8hobbies/typedoc-plugin-404"
  },
  "keywords": [
    "finance"
  ],
  "author": "Hong Xu",
  "license": " Apache-2.0",
  "bugs": {
    "url": "https://gitlab.com/8hobbies/irr/issues"
  },
  "homepage": "https://typedoc-plugin-404.8hobbies.com",
  "devDependencies": {
    "@types/node": "^20.11.28",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.2.5",
    "typescript": "^5.4.2",
    "vitest": "^1.4.0"
  },
  "dependencies": {
    "typedoc": "^0.25.12"
  }
}
